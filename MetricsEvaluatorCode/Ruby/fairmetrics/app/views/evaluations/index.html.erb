<h1>Evaluations</h1>

<table>
  <thead>
    <tr>
      <th>Collection</th>
      <th>Resource</th>
<!--       <th>Body</th>
      <th>Result</th> -->
      <th>Executor</th>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @evaluations.sort_by {|e| e.resource}.each do |evaluation| %>
      <tr>
        <td><%= evaluation.collection %></td>
        <td><%= evaluation.resource %></td>
<!--        <td><%= evaluation.body %></td>
        <td><%= evaluation.result %></td> -->
        <td><%= evaluation.executor %></td>
        <td><%= evaluation.title %></td>
        <td><%= link_to 'See Result', getresult_path(evaluation) %></td>

      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Evaluation', new_evaluation_path %>
<%= link_to 'Home', about_path %>
